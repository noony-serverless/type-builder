{
  "name": "@ultra-fast-builder/core",
  "version": "1.0.0",
  "description": "Ultra-fast TypeScript builder library with auto-detection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "benchmark": "node dist/benchmark.js",
    "clinic": "clinic doctor -- node dist/benchmark.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "clinic": "^12.0.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "keywords": [
    "builder",
    "typescript",
    "performance",
    "zod",
    "class",
    "interface",
    "validation"
  ]
}
