{
  "name": "@noony-serverless/type-builder",
  "version": "1.2.1",
  "description": "Ultra-fast TypeScript builder library with auto-detection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Noony Serverless",
  "license": "MIT",
  "private": false,
  "homepage": "https://noony-serverless.github.io/type-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noony-serverless/type-builder.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "clinic": "^12.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "builder",
    "typescript",
    "performance",
    "zod",
    "class",
    "interface",
    "validation",
    "functional-programming",
    "fp",
    "monad",
    "either",
    "maybe",
    "lens",
    "prism",
    "immutable",
    "optics"
  ]
}
